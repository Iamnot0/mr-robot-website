{
  "version": 2,
  "builds": [
    {
      "src": "fronted/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "fronted/build"
      }
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/fronted/index.html"
    }
  ]
}
